{
  "name": "la-county-polling-crawler",
  "timeout": 240,
  "version": "1.0.0",
  "description": "Simple LA County Web Crawling Application Used as a Backend for Mobile Clients",
  "scripts": {
    "start": "babel-watch ./src/server.js",
    "production": "NODE_ENV=production node ./dist/server.js",
    "lint": "npm-run-all --parallel eslint",
    "eslint": "eslint \"src/**\" --color",
    "test": "jest --silent",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --runInBand --ci",
    "remove-dist": "rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm run clean-dist",
    "build": "babel ./src --out-dir ./dist --ignore spec.js --copy-files;",
    "build:crawler": "npm-run-all build deploy:crawler",
    "deploy:crawler": "sh deploy.sh crawler crawler"
  },
  "author": "Oleg Shalygin",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-regenerator-runtime": "6.5.0",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "babel-template": "6.26.0",
    "babel-watch": "2.0.7",
    "body-parser": "1.18.3",
    "caniuse-api": "2.0.0",
    "caniuse-db": "1.0.30000830",
    "caniuse-lite": "1.0.30000827",
    "cheerio": "^1.0.0-rc.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cross-env": "3.0.0",
    "date-fns": "1.29.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-prettier": "2.6.1",
    "eslint-watch": "3.1.4",
    "eventsource-polyfill": "0.9.6",
    "express": "4.16.3",
    "express-winston": "2.4.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.3",
    "jwt-decode": "2.2.0",
    "npm-run-all": "4.1.2",
    "prettier": "1.13.5",
    "puppeteer": "1.8.0",
    "ramda": "0.24.1",
    "resolve-url": "0.2.1",
    "rimraf": "2.6.2",
    "shuffle-array": "1.0.1",
    "sinon": "3.0.0",
    "source-map-support": "0.4.15",
    "universal-cookie": "2.0.8",
    "url-loader": "1.0.1",
    "uuid": "3.0.1",
    "winston": "2.3.1",
    "winston-express": "0.1.1"
  },
  "devDependencies": {
    "jest-junit": "3.7.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testRegex": ".*?(\\.spec).js",
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "client/**/*.js",
      "client/**/*.jsx",
      "server/**/*.js*"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest/build/index.js"
    },
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./utilities/testSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/utilities/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coverageReporters": [
      "json",
      "html",
      "text",
      "lcov",
      "clover"
    ]
  }
}
